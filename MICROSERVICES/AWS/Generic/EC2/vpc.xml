    <variable displayName="CIDR Block" displayOrder="1" maxLength="100" name="params.cidr_block" startIncrement="0" type="String"/>
    <variable displayName="State" displayOrder="2" maxLength="100" name="params.state" startIncrement="0" type="String"/>
